{
  "name": "recommendations-pwa-collector",
  "version": "0.0.0-development",
  "main": "./lib/index.js",
  "license": "MIT",
  "pwa-studio": {
    "targets": {
      "intercept": "./lib/targets/intercept.js"
    }
  },
  "scripts": {
    "format": "npm run prettier -- --write",
    "lint": "eslint 'lib/**/*'",
    "lint:fix": "npm run lint -- --fix",
    "lint-staged": "lint-staged",
    "prettier": "prettier \"**/*.+(ts|tsx|js|json)\"",
    "tessa": "tessa-npm-plugin check --errorOnNewVulnerability",
    "tessa:update": "tessa-npm-plugin update",
    "test": "echo \"Error: no test specified\"",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "@magento/peregrine": "~6.0.0",
    "@magento/venia-ui": "~6.0.0",
    "react": "~16.9.0",
    "react-router-dom": "~5.1.0"
  },
  "devDependencies": {
    "@adobe/tessa-npm-plugin": "1.4.2",
    "@digitalroute/cz-conventional-changelog-for-jira": "6.0.2",
    "commitizen": "4.2.2",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "semantic-release": "^17.2.1"
  },
  "lint-staged": {
    "**/*.+(ts|tsx|js)": [
      "npm run lint:fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira"
    }
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/magento-commerce/data-services-pwa-recs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "develop",
        "prerelease": "beta"
      }
    ]
  }
}
